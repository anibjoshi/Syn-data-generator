{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": ["react"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "no-unused-vars": "error"
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "parser": "@typescript-eslint/parser",
      "plugins": ["@typescript-eslint"],
      "extends": ["plugin:@typescript-eslint/recommended"]
    }
  ]
}